<% form_for @lab_test_result do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :value %><br />
    <% if f. %>
      <%= f.text_field :value %>
    <% else %>
      ...
    <% end %>
  </p>
  <p>
    <%= f.label :lab_test_id %><br />
    <%= f.collection_select :lab_test_id, LabTest.all, :id, :name %>
  </p>
  <p>
    <%= f.label :accession_id %><br />
    <%= f.text_field :accession_id %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
