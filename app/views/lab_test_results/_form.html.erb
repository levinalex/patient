<% form_for @lab_test_result do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :value %><br />
    <%= f.text_field :value %>
  </p>
  <p>
    <%= f.label :lab_test_id %><br />
    <%= f.text_field :lab_test_id %>
  </p>
  <p>
    <%= f.label :accession_id %><br />
    <%= f.text_field :accession_id %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
