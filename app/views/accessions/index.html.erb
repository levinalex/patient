    <%= link_to "Order Tests", new_patient_accession_path(@patient) %>
  </div>
  <div class='title'><%=h @patient.full_name if @patient %></div>
<% end %>

<div class='list' id="requisitions">
  <%= render "accessions" %>
</div>
