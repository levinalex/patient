<% labeled_form_for @lab_test do |f| %>
  <%= f.error_messages %>
  <%= f.collection_select :lab_test_department_id, LabTestDepartment.all, :id, :name %>
  <%= f.text_field :code %>
  <%= f.text_field :name %>
  <%= f.collection_select :lab_test_unit_id, LabTestUnit.all, :id, :name, :prompt => "Select" %>
  <%= f.text_field :procedure %>
  <%= f.text_area :description %>
  <%= f.text_field :decimals %>
  <%= f.check_box :derivation, :label => "Derivation" %>
  <%= f.check_box :also_numeric, :label => "Also allow numeric values?"%>
  <%= f.check_box :ratio, :label => "N:N" %>
  <%= f.check_box :range, :label => "Nâ€“N" %>
  <%= f.check_box :fraction, :label => "N/N" %>
  <%= f.text_field :text_length %>
  <%= f.submit "Submit" %>
<% end %>
